<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/exampleFragment">

    <fragment
        android:id="@+id/exampleFragment"
        android:name="com.example.retrofitdemo.ui.fragments.ExampleFragment"
        android:label="fragment_example"
        tools:layout="@layout/fragment_example" >
        <action
            android:id="@+id/toFullDataFragment"
            app:destination="@id/fullDataFragment" />
    </fragment>
    <fragment
        android:id="@+id/fullDataFragment"
        android:name="com.example.retrofitdemo.ui.fragments.FullDataFragment"
        android:label="fragment_full_data"
        tools:layout="@layout/fragment_full_data" />
</navigation>